<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        {% block body %}
            {{ form_start(form, {'method': 'POST'}) }}
                {{form_row(form.products)}}
                {{form_row(form.submit, { 'label': 'Submit me' })}}
            {{ form_end(form) }}
        {% endblock %}
    </body>
</html>
